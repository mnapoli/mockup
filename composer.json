{
    "name": "mnapoli/mockup",
    "description": "Concise mock library for PHP tests",
    "keywords": [],
    "homepage": "https://github.com/mnapoli/mockup",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Mockup\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mockup\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^5.5|^7.0",
        "ocramius/proxy-manager": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    }
}
